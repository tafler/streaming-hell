{
  "name": "streaming-hell",
  "version": "0.1.1",
  "description": "Telegram bot for share music with anyone no matter the platform",
  "keywords": ["telegram", "telegram bot", "telegraf"],
  "homepage": "https://github.com/bukhalo/streaming-hell#readme",
  "bugs": {
    "url" : "https://github.com/bukhalo/streaming-hell/issues",
